{
    "$schema": "https://raw.githubusercontent.com/Nick2bad4u/OhMyPosh-Atomic-Enhanced/refs/heads/main/secrets.schema.json",
    "$id": "https://github.com/Nick2bad4u/OhMyPosh-Atomic-Enhanced/blob/main/secrets.schema.json",
    "$comment": "Copy this file to secrets.json (gitignored) and fill in your real keys. Never commit secrets.json.",
    "lastfm": {
        "api_key": "YOUR_LASTFM_API_KEY",
        "username": "YOUR_LASTFM_USERNAME",
        "http_timeout": 1500
    },
    "owm": {
        "api_key": "YOUR_OPENWEATHERMAP_API_KEY",
        "units": "imperial",
        "http_timeout": 1000,
        "location": "MANHATTEN,NY,US"
    },
    "brewfather": {
        "user_id": "YOUR_BREWFATHER_USER_ID",
        "api_key": "YOUR_BREWFATHER_API_KEY"
    },
    "wakatime": {
        "api_key": "YOUR_WAKATIME_API_KEY"
    },
    "strava": {
        "access_token": "YOUR_STRAVA_ACCESS_TOKEN",
        "refresh_token": "YOUR_STRAVA_REFRESH_TOKEN",
        "expires_in": 0
    },
    "withings": {
        "access_token": "YOUR_WITHINGS_ACCESS_TOKEN",
        "refresh_token": "YOUR_WITHINGS_REFRESH_TOKEN",
        "expires_in": 0
    }
}
