            "branch_icon": "Óú• ",
            "fetch_status": true,
            "fetch_upstream_icon": true
          },
          "style": "plain",
          "template": "<p:white>[</>{{ .HEAD }}{{ if .Staging.Changed }}<p:green_added> ‚óè {{ .Staging.String }}</>{{ end }}{{ if .Working.Changed }}<p:orange> ‚óè {{ .Working.String }}</>{{ end }}<p:white>]-</>",
          "type": "git"
        },
        {
          "foreground": "p:green_success",
          "foreground_templates": [
            "{{ if gt .Code 0 }}p:red_alert{{ end }}"
          ],
          "properties": {
            "always_enabled": true
          },
          "style": "plain",
          "template": "ÓäÖÓÆû ",
          "type": "status"
        }
      ],
      "type": "prompt"
