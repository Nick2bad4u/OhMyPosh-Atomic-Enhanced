  "blocks": [
    {
      "alignment": "left",
      "segments": [
        {
          "style": "plain",
          "template": "‚ï≠‚îÄ[<p:yellow_bright>{{ .UserName }}</><p:red_alert>@</><p:green_ahead>{{ .HostName }}</>]‚îÄ",
          "type": "session"
