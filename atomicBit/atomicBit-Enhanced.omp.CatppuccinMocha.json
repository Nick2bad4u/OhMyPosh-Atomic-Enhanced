        {
          "foreground": "p:blue_time",
          "properties": {
            "folder_separator_icon": " ÔÅ° ",
            "mapped_locations": {
              "re:.*(Desktop).*": "ÔÑà DT",
              "re:.*(Documents).*": "ÔÄ≠ Docs",
              "re:.*(Downloads).*": "ÔÄô DL",
              "re:.*(GitHub).*": "ÔÇõ GH",
              "re:.*(\\.git).*": "Óú• .git",
              "re:.*(node_modules).*": "Óúò nm",
              "~": "Ó¨Ü ~"
            },
            "mapped_locations_enabled": true
          },
          "style": "folder",
          "template": "<p:white>[</>{{ .Path }}<p:white>]</>",
          "type": "path"
        },
        {
          "foreground": "p:orange",
          "style": "plain",
          "template": "<p:white>‚îÄ(</>#<p:white>)</>",
          "type": "root"
        }
      ],
      "type": "prompt"
    },
    {
      "alignment": "right",
      "segments": [
        {
          "foreground": "p:node_green",
          "properties": {
            "fetch_package_manager": true,
            "npm_icon": " <p:red_alert>Óó∫</> ",
            "yarn_icon": " <p:blue_primary>Óöß</>"
          },
          "style": "plain",
          "template": "<p:white>(</>{{ if .PackageManagerIcon }}{{ .PackageManagerIcon }} {{ end }}{{ .Full }}<p:white>)</>",
          "type": "node"
        },
        {
          "foreground": "p:python_yellow",
          "style": "plain",
          "template": "<p:white>(</>{{ if .Error }}{{ .Error }}{{ else }}{{ if .Venv }}{{ .Venv }} {{ end }}{{ .Full }}{{ end }}<p:white>)</>",
          "type": "python"
        },
        {
          "foreground": "p:java_orange",
          "style": "plain",
          "template": "<p:white>(</>{{ if .Error }}{{ .Error }}{{ else }}{{ .Full }}{{ end }}<p:white>)</>",
          "type": "java"
        },
        {
          "foreground": "p:typescript_blue",
          "style": "plain",
          "template": "<p:white>(</>{{ if .Unsupported }}ÔÅ±{{ else }}{{ .Full }}{{ end }}<p:white>)</>",
          "type": "dotnet"
        },
        {
          "foreground": "#06aad5",
          "style": "plain",
          "template": "<p:white>(</>{{ if .Error }}{{ .Error }}{{ else }}{{ .Full }}{{ end }}<p:white>)</>",
          "type": "go"
        },
        {
          "foreground": "#925837",
          "style": "plain",
          "template": "<p:white>(</>{{ if .Error }}{{ .Error }}{{ else }}{{ .Full }}{{ end }}<p:white>)</>",
          "type": "rust"
        },
        {
          "foreground": "p:react_cyan",
          "style": "plain",
          "template": "<p:white>(</>{{ if .Error }}{{ .Error }}{{ else }}{{ .Full }}{{ end }}<p:white>)</>",
          "type": "angular"
        },
        {
          "foreground": "p:white",
          "properties": {
            "linux": "<p:white>Óâø</>",
            "macos": "<p:white>Óâø</>",
            "windows": "<p:white>Óâø</>"
          },
          "style": "plain",
          "template": "<p:white>(</>{{ if .WSL }}WSL at {{ end }}{{.Icon}}<p:white>)‚îÄ</>",
          "type": "os"
        },
        {
          "foreground": "p:white",
          "foreground_templates": [
            "{{if eq \"Charging\" .State.String}}p:green_charging{{end}}",
            "{{if eq \"Discharging\" .State.String}}p:yellow_discharging{{end}}",
            "{{if eq \"Full\" .State.String}}p:green_full{{end}}"
          ],
          "properties": {
            "charged_icon": "ÔâÄ ",
            "charging_icon": "Ôá¶ ",
            "discharging_icon": "Óà¥ "
          },
          "style": "plain",
          "template": "<p:white>[</>{{ if not .Error }}{{ .Icon }}{{ .Percentage }}{{ end }}{{ .Error }}%<p:white>]‚îÄ</>",
          "type": "battery"
        },
        {
          "foreground": "p:blue_time",
          "properties": {
            "time_format": "_2,15:04"
          },
          "style": "plain",
          "template": "<p:white>[</>ÔÅ≥{{ .CurrentDate | date .Format }}<p:white>]</>",
          "type": "time"
        }
      ],
      "type": "prompt"
    },
    {
      "alignment": "left",
      "newline": true,
      "segments": [
        {
          "style": "plain",
          "template": "‚ï∞‚îÄ",
          "type": "text"
        },
        {
          "foreground": "p:purple_ahead",
