          "foreground": "p:blue_time",
          "properties": {
            "folder_separator_icon": " ÔÅ° ",
            "mapped_locations": {
              "re:.*(Desktop).*": "ÔÑà DT",
              "re:.*(Documents).*": "ÔÄ≠ Docs",
              "re:.*(Downloads).*": "ÔÄô DL",
              "re:.*(GitHub).*": "ÔÇõ GH",
              "re:.*(\\.git).*": "Óú• .git",
              "re:.*(node_modules).*": "Óúò nm",
              "~": "Ó¨Ü ~"
