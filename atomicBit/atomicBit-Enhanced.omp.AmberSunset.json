        {
          "foreground": "p:green_success",
          "foreground_templates": ["{{ if gt .Code 0 }}p:red_alert{{ end }}"],
          "properties": {
            "always_enabled": true
          },
          "style": "plain",
          "template": "ÓäÖÓÆû ",
          "type": "status"
