          "style": "folder",
          "template": "<p:white>[</>{{ .Path }}<p:white>]</>",
          "type": "path"
        },
        {
          "foreground": "p:orange",
          "style": "plain",
          "template": "<p:white>‚îÄ(</>#<p:white>)</>",
          "type": "root"
        }
