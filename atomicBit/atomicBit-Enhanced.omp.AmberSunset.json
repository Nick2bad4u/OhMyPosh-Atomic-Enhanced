          "foreground": "p:white",
          "properties": {
            "linux": "<p:white>Óâø</>",
            "macos": "<p:white>Óâø</>",
            "windows": "<p:white>Óâø</>"
          },
          "style": "plain",
          "template": "<p:white>(</>{{ if .WSL }}WSL at {{ end }}{{.Icon}}<p:white>)‚îÄ</>",
          "type": "os"
        },
        {
          "foreground": "p:white",
          "foreground_templates": [
            "{{if eq \"Charging\" .State.String}}p:green_charging{{end}}",
            "{{if eq \"Discharging\" .State.String}}p:yellow_discharging{{end}}",
            "{{if eq \"Full\" .State.String}}p:green_full{{end}}"
          ],
          "properties": {
            "charged_icon": "ÔâÄ ",
            "charging_icon": "Ôá¶ ",
            "discharging_icon": "Óà¥ "
          },
          "style": "plain",
          "template": "<p:white>[</>{{ if not .Error }}{{ .Icon }}{{ .Percentage }}{{ end }}{{ .Error }}%<p:white>]‚îÄ</>",
          "type": "battery"
        },
        {
          "foreground": "p:blue_time",
          "properties": {
            "time_format": "_2,15:04"
          },
          "style": "plain",
          "template": "<p:white>[</>ÔÅ≥{{ .CurrentDate | date .Format }}<p:white>]</>",
          "type": "time"
        }
