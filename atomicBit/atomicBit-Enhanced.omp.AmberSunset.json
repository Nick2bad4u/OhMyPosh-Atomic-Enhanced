          "style": "plain",
          "template": "<p:white>(</>{{ if .PackageManagerIcon }}{{ .PackageManagerIcon }} {{ end }}{{ .Full }}<p:white>)</>",
          "type": "node"
        },
        {
          "foreground": "p:python_yellow",
          "style": "plain",
          "template": "<p:white>(</>{{ if .Error }}{{ .Error }}{{ else }}{{ if .Venv }}{{ .Venv }} {{ end }}{{ .Full }}{{ end }}<p:white>)</>",
          "type": "python"
