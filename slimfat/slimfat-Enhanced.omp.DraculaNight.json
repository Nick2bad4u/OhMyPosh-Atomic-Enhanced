  "accent_color": "p:accent",
  "async": true,
  "blocks": [
    {
      "alignment": "left",
      "segments": [
        {
          "background": "p:gray_prompt_count_bg",
          "foreground": "p:blue_time",
          "leading_diamond": "ÓÉá‚ñì",
          "min_width": 80,
          "properties": {
            "alpine": "ÔåÄ",
            "arch": "ÔåÉ",
            "centos": "ÔåÑ",
            "debian": "ÔåÜ",
            "elementary": "Ôåâ",
            "fedora": "Ôåä",
            "gentoo": "Ôåç",
            "linux": "Óúí",
            "macos": "Óúë",
            "manjaro": "Ôåí",
            "mint": "Ôåè",
            "opensuse": "Ôåî",
            "raspbian": "Ôåï",
            "ubuntu": "Ôåú",
            "windows": "Óúè"
          },
          "style": "diamond",
          "template": " {{ if .WSL }}Óúí on {{ end }}{{ .Icon }}<p:gray_untracked> ÓÇ±</>",
          "type": "os"
        },
        {
          "background": "p:gray_prompt_count_bg",
          "foreground": "p:white",
          "style": "diamond",
          "template": " {{ if ne .Env.POSH_SESSION_DEFAULT_USER .UserName }}<p:green_ahead>{{ .UserName }}</><p:gray_untracked>Ôá∫</>{{ end }}<p:accent>{{ .HostName }}</><p:gray_untracked> ÓÇ±</>",
          "type": "session"
        },
        {
          "background": "p:gray_prompt_count_bg",
          "foreground": "p:yellow_bright",
          "style": "diamond",
          "template": " ÔÉß<p:gray_untracked> ÓÇ±</>",
          "type": "root"
        },
        {
          "background": "p:gray_prompt_count_bg",
          "foreground": "p:blue_time",
          "properties": {
            "folder_icon": "Óóæ ",
            "mapped_locations": {
              "re:.*(Desktop).*": "ÔÑà DT",
              "re:.*(Documents).*": "ÔÄ≠ Docs",
              "re:.*(Downloads).*": "ÔÄô DL",
              "re:.*(GitHub).*": "ÔÇõ GH",
              "re:.*(\\.git).*": "Óú• .git",
              "re:.*(node_modules).*": "Óúò node_modules",
              "~": "Ó¨Ü ~"
            },
            "mapped_locations_enabled": true,
            "style": "folder"
          },
          "style": "diamond",
          "template": " <p:yellow_bright>Óóø</> {{ .Path }} ",
          "type": "path"
        },
        {
          "background": "p:gray_prompt_count_bg",
          "foreground": "p:yellow_modified",
          "foreground_templates": [
            "{{ if or (.Working.Changed) (.Staging.Changed) }}p:yellow_modified{{ end }}",
            "{{ if gt .Ahead 0 }}p:green_ahead{{ end }}",
            "{{ if gt .Behind 0 }}p:purple_ahead{{ end }}"
          ],
          "properties": {
            "fetch_status": true,
            "fetch_upstream_icon": true
          },
          "style": "diamond",
          "template": "<p:gray_untracked>ÓÇ± </>{{ .UpstreamIcon }} {{ .HEAD }}{{if .BranchStatus }} {{ .BranchStatus }}{{ end }}{{ if .Working.Changed }}<p:red_alert> ÔÅÑ {{ .Working.String }}</>{{ end }}{{ if and (.Working.Changed) (.Staging.Changed) }} |{{ end }}{{ if .Staging.Changed }}<p:green_added> ÔÅÜ {{ .Staging.String }}</>{{ end }}{{ if gt .StashCount 0 }} Ó≠ã {{ .StashCount }}{{ end }} ",
          "type": "git"
        },
        {
          "background": "p:gray_prompt_count_bg",
          "foreground": "p:white",
          "style": "diamond",
          "trailing_diamond": "ÓÇ∞",
          "type": "text"
        }
      ],
      "type": "prompt"
    },
    {
      "alignment": "right",
      "segments": [
        {
          "background": "p:gray_prompt_count_bg",
          "foreground": "p:white",
          "leading_diamond": "ÓÇ≤",
          "style": "diamond",
          "type": "text"
        },
        {
          "background": "p:gray_prompt_count_bg",
          "foreground": "p:node_green",
          "style": "diamond",
          "template": " Óúò {{ if .PackageManagerIcon }}{{ .PackageManagerIcon }} {{ end }}{{ .Full }}<p:gray_untracked> ÓÇ≥</>",
          "type": "node"
        },
        {
          "background": "p:gray_prompt_count_bg",
          "foreground": "p:python_blue",
          "style": "diamond",
          "template": " Óàµ {{ if .Error }}{{ .Error }}{{ else }}{{ if .Venv }}{{ .Venv }} {{ end }}{{ .Full }}{{ end }}<p:gray_untracked> ÓÇ≥</>",
          "type": "python"
        },
        {
          "background": "p:gray_prompt_count_bg",
          "foreground": "p:typescript_blue",
          "style": "diamond",
          "template": " Óùø {{ if .Unsupported }}ÔÅ±{{ else }}{{ .Full }}{{ end }}<p:gray_untracked> ÓÇ≥</>",
          "type": "dotnet"
        },
        {
          "background": "p:gray_prompt_count_bg",
          "foreground": "p:white",
          "properties": {
            "time_format": "15:04:05"
          },
          "style": "diamond",
          "template": " {{ .CurrentDate | date .Format }}<p:blue_primary> ÓôÅ </>",
          "trailing_diamond": "‚ñìÓÉÜ",
          "type": "time"
        }
      ],
      "type": "prompt"
    },
    {
      "alignment": "left",
      "newline": true,
      "segments": [
        {
          "foreground": "p:gray_untracked",
          "style": "plain",
          "template": " ~#@‚ùØ ",
          "type": "text"
        }
      ],
      "type": "prompt"
    },
    {
      "alignment": "right",
      "segments": [
        {
          "foreground": "p:gray_untracked",
          "style": "plain",
          "template": " ‚ùÆ ",
          "type": "text"
        },
        {
          "foreground": "p:yellow_bright",
          "properties": {
            "style": "roundrock",
            "threshold": 500
          },
          "style": "plain",
          "template": " {{ .FormattedMs }}<p:white> Ôâí </>",
          "type": "executiontime"
        },
        {
          "foreground": "p:green_success",
          "foreground_templates": [
            "{{ if gt .Code 0 }}p:red_alert{{ end }}"
          ],
          "properties": {
            "always_enabled": true
          },
          "style": "plain",
          "template": " Ó™ü {{ if gt .Code 0 }}{{ .Code }}{{ end }} ",
          "type": "status"
        }
      ],
      "type": "rprompt"
    }
