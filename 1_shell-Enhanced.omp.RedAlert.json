{
  "$schema": "https://raw.githubusercontent.com/JanDeDobbeleer/oh-my-posh/main/themes/schema.json",
  "accent_color": "p:accent",
  "async": true,
  "blocks": [
    {
      "alignment": "left",
      "newline": true,
      "segments": [
        {
          "foreground": "p:pink_weather",
          "leading_diamond": "<p:pink_storybook>  </>",
          "properties": {
            "display_host": true
          },
          "style": "diamond",
          "template": "{{ .UserName }} <p:white>on</>",
          "type": "session"
        },
        {
          "foreground": "p:purple_session",
          "properties": {
            "time_format": "Monday <p:white>at</> 3:04 PM"
          },
          "style": "diamond",
          "template": " {{ .CurrentDate | date .Format }} ",
          "type": "time"
        },
        {
          "foreground": "p:purple_ahead",
          "properties": {
            "branch_icon": " ",
            "fetch_status": true,
            "fetch_upstream_icon": true,
            "fetch_worktree_count": true
          },
          "style": "diamond",
          "template": " {{ .UpstreamIcon }}{{ .HEAD }}{{if .BranchStatus }} {{ .BranchStatus }}{{ end }}{{ if .Working.Changed }}  {{ .Working.String }}{{ end }}{{ if and (.Working.Changed) (.Staging.Changed) }} |{{ end }}{{ if .Staging.Changed }}  {{ .Staging.String }}{{ end }}{{ if gt .StashCount 0 }}  {{ .StashCount }}{{ end }} ",
          "type": "git"
        }
      ],
      "type": "prompt"
    },
    {
      "alignment": "right",
      "segments": [
        {
          "foreground": "p:green_charging",
          "properties": {
            "style": "dallas",
            "threshold": 0
          },
          "style": "diamond",
          "template": " {{ .FormattedMs }}s <p:white></>",
          "type": "executiontime"
        },
        {
          "properties": {
            "root_icon": " "
          },
          "style": "diamond",
          "template": "  ",
          "type": "root"
        },
        {
          "foreground": "p:green_ahead",
          "style": "diamond",
          "template": " <p:white>MEM:</> {{ round .PhysicalPercentUsed .Precision }}% ({{ (div ((sub .PhysicalTotalMemory .PhysicalAvailableMemory)|float64) 1073741824.0) }}/{{ (div .PhysicalTotalMemory 1073741824.0) }}GB)",
          "type": "sysinfo"
        }
      ],
      "type": "prompt"
    },
    {
      "alignment": "left",
      "newline": true,
      "segments": [
        {
          "foreground": "p:pink_weather",
          "leading_diamond": "<p:blue_time>  </><p:pink_weather>{</>",
          "properties": {
            "folder_icon": "",
            "folder_separator_icon": "  ",
            "home_icon": "home",
            "style": "folder",
            "mapped_locations": {
              "re:.*(Uptime-Watcher).*": "UW",
              "re:.*(GitHub).*": "GH",
              "re:.*(Documents).*": "Docs",
              "re:.*(Downloads).*": "DLs",
              "re:.*(OneDrive).*": "OD",
              "re:.*(Code).*": "Code",
              "re:.*(Projects).*": "Proj",
              "re:.*(Workspace).*": "WS",
              "re:.*(Desktop).*": "DT",
              "re:.*(Pictures).*": "Pics",
              "re:.*(Music).*": "Music",
              "re:.*(Videos).*": "Videos",
              "re:.*(nodejs).*": "Node",
              "re:.*(npm).*": "NPM",
              "re:.*(yarn).*": "Yarn",
              "re:.*(pnpm).*": "PNPM",
              "re:.*(Dropbox).*": "DB",
              "re:.*(Google Drive).*": "GD",
              "re:.*(iCloud).*": "IC",
              "re:.*(Library).*": "Lib",
              "re:.*(node_modules).*": "📚 ",
              "re:.*(\\.git).*": "🌳 ",
              "re:.*(\\.github).*": "🐙 ",
              "re:.*(auth|security).*": "🔐 ",
              "re:.*(ci|cd|pipeline).*": "🔄 ",
              "re:.*(gitlab|bitbucket).*": "🐙 ",
              "re:.*(wintertodt-scouter).*": "❄️ WintTodt",
              "re:.*(GE-Filters).*": "💰 GE-Flt",
              "re:.*(plugin-hub).*": "🪝 PlugHub",
              "re:.*(internet-archive-upload).*": "🗄️ Archive",
              "re:.*(github-drama).*": "🎭 Drama",
              "re:.*(MarkOfDarknessHelper).*": "🌑 MOD-Help",
              "re:.*(generate-repo-file-list).*": "🗂️ GenList",
              "re:.*(thrall-helper).*": "👹 Thrall",
              "re:.*(BH-Scripts).*": "🎬 BH",
              "re:.*(DemoMan-Fork|DemoMan).*": "🎥 DemoMan",
              "re:.*(LocalHistory).*": "⏰ LocHist",
              "re:.*(wiki-bank-tag-integration).*": "🏦 BankTag",
              "re:.*(Sauce4ZwiftModsRepo).*": "🚴 Zwift",
              "re:.*(ZwiftScripts).*": "📜 ZScript",
              "re:.*(UserStyles).*": "🖼️ Styles",
              "re:.*(tf2).*": "🎮 TF2",
              "re:.*(FitFileViewer).*": "🗂️ FitView",
              "re:.*(ps-color-scripts-enhanced|ps-color-scripts).*": "🎨 PSColor",
              "re:.*(OhMyPosh-Atomic-Enhanced).*": "✨ OhMyPosh",
              "re:.*(Typpis-ESLint).*": "🔍 Typpis",
              "re:.*(worktree-backup).*": "🌳 WTBackup",
              "re:.*(git-backup).*": "💾 GitBkup",
              "re:.*(dnsValidator).*": "🌐 DNS",
              "re:.*(eslint-plugin-chunkyLint).*": "📏 ChunkyL",
              "re:.*(eslint-config-Typescript-Extreme).*": "⚡ TSExtm",
              "re:.*(runelite.phyce.dev).*": "🔮 Runite",
              "re:.*(Nick2bad4u).*": "👤 Profile",
              "C:\\": "C:",
              "~/": "~",
              "~\\Desktop": "🖥️ ",
              "~\\Documents": "📄 ",
              "~\\Downloads": "📥 "
            },
            "mapped_locations_enabled": true
          },
          "style": "diamond",
          "template": "  {{ .Path }} ",
          "trailing_diamond": "<p:pink_weather>}</>",
          "type": "path"
        },
        {
          "foreground": "p:green_success",
          "foreground_templates": [
            "{{ if gt .Code 0 }}p:red_alert{{ end }}"
          ],
          "properties": {
            "always_enabled": true
          },
          "style": "plain",
          "template": "  ",
          "type": "status"
        }
      ],
      "type": "prompt"
    }
  ],
  "console_title_template": "{{.Folder}}{{if .Root}} : root{{end}} : {{.Shell}}",
  "debug_prompt": {
    "background": "transparent",
    "foreground": "p:black",
    "template": "</><p:purple_session,transparent></><p:black,p:purple_session>  DEBUG </><p:purple_session,transparent></>"
