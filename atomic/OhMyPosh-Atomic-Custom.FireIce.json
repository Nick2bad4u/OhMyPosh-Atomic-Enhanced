{
  "$schema": "https://raw.githubusercontent.com/JanDeDobbeleer/oh-my-posh/main/themes/schema.json",
  "accent_color": "#21c7c7",
  "async": true,
  "terminal_background": "p:black",
  "blocks": [
    {
      "alignment": "left",
      "segments": [
        {
          "background": "p:blue_primary",
          "cache": {
            "duration": "1h",
            "strategy": "session"
          },
          "foreground": "p:white",
          "leading_diamond": "╭─",
          "min_width": 80,
          "properties": {
            "mapped_shell_names": {
              "bash": "BASH",
              "cmd": "CMD",
              "fish": "FISH",
              "powershell": "PS",
              "ps": "PS",
              "pwsh": "PS",
              "zsh": "ZSH"
            }
          },
          "style": "diamond",
          "template": " {{ .Name  }} {{ .Version }} ",
          "type": "shell"
        },
        {
          "background": "p:red_alert",
          "foreground": "p:black",
          "min_width": 80,
          "style": "diamond",
          "template": "<parentBackground></>  ",
          "type": "root"
        },
        {
          "background": "p:orange",
          "foreground": "p:black",
          "min_width": 80,
          "powerline_symbol": "",
          "properties": {
            "edge_format": "<b>%s</b>",
            "folder_format": "<b>%s</b>",
            "folder_icon": "  ",
            "folder_separator_icon": "/",
            "gitdir_format": "<d>%s</d>",
            "home_icon": "",
            "mapped_locations": {
              "C:\\": "C:",
              "re:.*(BH-Scripts).*": "🎬 BH",
              "re:.*(Code).*": "Code",
              "re:.*(DemoMan-Fork|DemoMan).*": "🎥 DemoMan",
              "re:.*(Desktop).*": "DT",
              "re:.*(Documents).*": "Docs",
              "re:.*(Downloads).*": "DLs",
              "re:.*(Dropbox).*": "DB",
              "re:.*(FitFileViewer).*": "🗂️ FitView",
              "re:.*(GE-Filters).*": "💰 GE-Flt",
              "re:.*(GitHub).*": "GH",
              "re:.*(Google Drive).*": "GD",
              "re:.*(Library).*": "Lib",
              "re:.*(LocalHistory).*": "⏰ LocHist",
              "re:.*(MarkOfDarknessHelper).*": "🌑 MOD-Help",
              "re:.*(Music).*": "Music",
              "re:.*(Nick2bad4u).*": "👤 Profile",
              "re:.*(OhMyPosh-Atomic-Enhanced).*": "✨ OhMyPosh",
              "re:.*(OneDrive).*": "OD",
              "re:.*(Pictures).*": "Pics",
              "re:.*(Projects).*": "Proj",
              "re:.*(Sauce4ZwiftModsRepo).*": "🚴 Zwift",
              "re:.*(Typpis-ESLint).*": "🔍 Typpis",
              "re:.*(Uptime-Watcher).*": "UW",
              "re:.*(UserStyles).*": "🖼️ Styles",
              "re:.*(Videos).*": "Videos",
              "re:.*(Workspace).*": "WS",
              "re:.*(ZwiftScripts).*": "📜 ZScript",
              "re:.*(\\.git).*": "🌳 ",
              "re:.*(\\.github).*": "🐙 ",
              "re:.*(auth|security).*": "🔐 ",
              "re:.*(ci|cd|pipeline).*": "🔄 ",
              "re:.*(dnsValidator).*": "🌐 DNS",
              "re:.*(eslint-config-Typescript-Extreme).*": "⚡ TSExtm",
              "re:.*(eslint-plugin-chunkyLint).*": "📏 ChunkyL",
              "re:.*(generate-repo-file-list).*": "🗂️ GenList",
              "re:.*(git-backup).*": "💾 GitBkup",
              "re:.*(github-drama).*": "🎭 Drama",
              "re:.*(gitlab|bitbucket).*": "🐙 ",
              "re:.*(iCloud).*": "IC",
              "re:.*(internet-archive-upload).*": "🗄️ Archive",
              "re:.*(node_modules).*": "📚 ",
              "re:.*(nodejs).*": "Node",
              "re:.*(npm).*": "NPM",
              "re:.*(plugin-hub).*": "🪝 PlugHub",
              "re:.*(pnpm).*": "PNPM",
              "re:.*(ps-color-scripts-enhanced|ps-color-scripts).*": "🎨 PSColor",
              "re:.*(runelite.phyce.dev).*": "🔮 Runite",
              "re:.*(tf2).*": "🎮 TF2",
              "re:.*(thrall-helper).*": "👹 Thrall",
              "re:.*(wiki-bank-tag-integration).*": "🏦 BankTag",
              "re:.*(wintertodt-scouter).*": "❄️ WintTodt",
              "re:.*(worktree-backup).*": "🌳 WTBackup",
              "re:.*(yarn).*": "Yarn",
              "~/": "~",
              "~\\\\Desktop": "🖥️ ",
              "~\\\\Documents": "📄 ",
              "~\\\\Downloads": "📥 "
            },
            "mapped_locations_enabled": true,
            "max_width": 40,
            "style": "folder"
          },
          "style": "powerline",
          "template": "  {{ .Path }} ",
          "type": "path"
        },
        {
          "background": "p:yellow_bright",
          "background_templates": [
            "{{ if or (.Working.Changed) (.Staging.Changed) }}p:yellow_git_changed{{ end }}",
            "{{ if and (gt .Ahead 0) (gt .Behind 0) }}p:green_ahead{{ end }}",
            "{{ if gt .Ahead 0 }}p:purple_ahead{{ end }}",
            "{{ if gt .Behind 0 }}p:purple_ahead{{ end }}"
          ],
          "cache": {
            "duration": "5m",
            "strategy": "folder"
          },
          "exclude_folders": [
            ".*node_modules.*",
            ".*\\.tmp.*",
            ".*\\.cache.*"
          ],
          "foreground": "p:navy_text",
          "include_folders": [
            ".*\\.git.*",
            ".*/.*",
            "~/.*"
          ],
          "min_width": 80,
          "powerline_symbol": "",
          "properties": {
            "branch_icon": "  ",
            "branch_template": "{{ if .Mapped }}{{ .Mapped }}{{ else }}{{ .Icon }}{{ end }}{{ .Branch }}",
            "fetch_bare_info": true,
            "fetch_stash_count": false,
            "fetch_status": false,
            "fetch_upstream_icon": true,
            "fetch_user": true,
            "status_formats": {
              "Added": "<p:green_added>+%d</>",
              "Copied": "<p:magenta_copied>c%d</>",
              "Deleted": "<p:red_deleted>-%d</>",
              "Modified": "<p:yellow_modified>~%d</>",
              "Renamed": "<p:cyan_renamed>r%d</>",
              "Unmerged": "<p:orange_unmerged>x%d</>",
              "Untracked": "<p:gray_untracked>?%d</>"
            },
            "untracked_modes": {
              "*": "normal"
            }
          },
          "style": "powerline",
          "template": " {{ .UpstreamIcon }}<b>{{ .HEAD }}</b>{{if .BranchStatus }} <d>{{ .BranchStatus }}</d>{{ end }} ",
          "type": "git"
        },
        {
          "background": "p:purple_exec",
          "cache": {
            "duration": "3s",
            "strategy": "session"
          },
          "foreground": "p:white",
          "min_width": 80,
          "properties": {
            "style": "roundrock",
            "threshold": 0
          },
          "style": "diamond",
          "template": " 󱑅 {{ .FormattedMs }}⠀",
          "trailing_diamond": "",
          "type": "executiontime"
        },
        {
          "background": "p:maroon_error",
          "background_templates": [
            "{{ if .Error }}p:maroon_error{{ end }}"
          ],
          "foreground": "p:black",
          "min_width": 80,
          "properties": {
            "always_enabled": false
          },
          "status_template": [
            "{{ if eq .Code 0 }}{{ else }}{{ end }}"
          ],
          "style": "diamond",
          "template": "<parentBackground></>  ",
          "trailing_diamond": "",
          "type": "status"
        }
      ],
      "type": "prompt"
    },
    {
      "alignment": "right",
      "filler": "",
      "overflow": "hide",
      "segments": [
        {
          "background": "p:teal_sysinfo",
          "cache": {
            "duration": "1m",
            "strategy": "session"
          },
          "foreground": "p:black",
          "invert_powerline": false,
          "leading_diamond": "",
          "min_width": 100,
          "powerline_symbol": "",
          "properties": {
            "precision": 1
          },
          "style": "diamond",
          "template": " 󰇅 {{ round .Load1 .Precision  }}%  {{ round .PhysicalPercentUsed .Precision  }}%  {{ round .Disks .Precision  }}% ",
          "type": "sysinfo"
        },
        {
          "background": "p:gray_os",
          "cache": {
            "duration": "5m",
            "strategy": "session"
          },
          "foreground": "p:gray_os_fg",
          "invert_powerline": false,
          "leading_diamond": "",
          "min_width": 100,
          "powerline_symbol": "",
          "properties": {
            "linux": "",
            "macos": "",
            "windows": ""
          },
          "style": "diamond",
          "template": " {{ if .WSL }}WSL at {{ end }}{{ .Icon }} ",
          "type": "os"
        },
        {
          "background": "p:blue_time",
          "foreground": "p:black",
          "invert_powerline": false,
          "leading_diamond": "",
          "min_width": 100,
          "powerline_symbol": "",
          "properties": {
            "time_format": "3:04 PM"
          },
          "style": "diamond",
          "template": "  {{ .CurrentDate | date .Format }} ",
          "type": "time"
        },
        {
          "background": "p:orange_battery",
          "background_templates": [
            "{{if eq \"Charging\" .State.String}}p:green_charging{{end}}",
            "{{if eq \"Discharging\" .State.String}}p:yellow_discharging{{end}}",
            "{{if eq \"Full\" .State.String}}p:green_full{{end}}"
          ],
          "cache": {
            "duration": "60m",
            "strategy": "session"
          },
          "foreground": "p:black",
          "invert_powerline": false,
          "leading_diamond": "",
          "min_width": 100,
          "powerline_symbol": "",
          "properties": {
            "charged_icon": "󱊣",
            "charging_icon": "󰂄",
            "discharging_icon": "󰂃",
            "display_errors": true,
            "not_charging_icon": "󰂂"
          },
          "style": "diamond",
          "template": " {{ if not .Error }}{{ .Icon }}{{ .Percentage }}{{ end }}{{ .Error }} ",
          "type": "battery"
        },
        {
          "background": "p:pink_weather",
          "cache": {
            "duration": "10m",
            "strategy": "session"
          },
          "foreground": "p:black",
          "invert_powerline": false,
          "leading_diamond": "",
          "min_width": 100,
          "powerline_symbol": "",
          "properties": {
            "http_timeout": 500,
            "units": "imperial"
          },
          "style": "diamond",
          "template": "<b>󰖐</b> {{.Temperature}}{{.UnitIcon}}",
          "trailing_diamond": "",
          "type": "owm"
        }
      ],
      "type": "prompt"
    },
    {
      "alignment": "right",
      "segments": [
        {
          "background": "p:gray_prompt_count_bg",
          "foreground": "p:gray_prompt_count_fg",
          "invert_powerline": false,
          "leading_diamond": "",
          "min_width": 80,
          "properties": {
            "always_enabled": true
          },
          "style": "diamond",
          "template": " ⌁ {{ .PromptCount }} ",
          "trailing_diamond": "",
          "type": "text"
        },
        {
          "background": "p:npm_yellow",
          "foreground": "p:npm_dark",
          "leading_diamond": "",
          "min_width": 80,
          "properties": {
            "cache_duration": "24h",
            "display_mode": "context",
            "fetch_version": true,
            "home_enabled": false,
            "strategy": "session",
            "missing_command_text": "",
            "version_url_template": "https://registry.npmjs.org/{{ .PackageName }}/latest"
          },
          "style": "diamond",
          "template": " <b> {{ .Full }}</b> ",
          "trailing_diamond": "",
          "type": "npm"
        },
        {
          "background": "#6CA35E",
          "foreground": "#ffffff",
          "leading_diamond": "",
          "min_width": 80,
          "properties": {
            "cache_duration": "24h",
            "strategy": "session",
            "display_mode": "context",
            "home_enabled": false,
            "fetch_version": true,
            "fetch_package_manager": true,
            "npm_icon": "",
            "version_url_template": "https://registry.npmjs.org/{{ .PackageName }}/latest",
            "missing_command_text": "",
            "yarn_icon": ""
          },
          "style": "diamond",
          "template": " <b> {{ .Full }}</b> ",
          "trailing_diamond": "",
          "type": "node"
        },
        {
          "background": "#4063D8",
          "foreground": "#ffffff",
          "leading_diamond": "",
          "min_width": 80,
          "properties": {
            "cache_duration": "24h",
            "strategy": "session",
            "display_mode": "context",
            "fetch_version": true,
            "home_enabled": false,
            "missing_command_text": "",
            "version_url_template": "https://api.adoptium.net/v3/info/release_names?version={{ .Major }}&os={{ .OS }}&arch={{ .Arch }}&image_type=jdk&jvm_impl=hotspot&vendor=eclipse"
          },
          "style": "diamond",
          "template": "  {{ .Full }}",
          "trailing_diamond": "",
          "type": "java"
        },
        {
          "background": "p:yellow_update",
          "cache": {
            "duration": "24h",
            "strategy": "session"
          },
          "foreground": "p:gray_os_fg",
          "invert_powerline": false,
          "leading_diamond": "",
          "min_width": 80,
          "properties": {
            "cache_duration": "24h",
            "strategy": "session"
          },
          "style": "diamond",
          "trailing_diamond": "",
          "type": "upgrade"
        },
        {
          "background": "p:yellow_root_alt",
          "cache": {
            "duration": "1m",
            "strategy": "session"
          },
          "foreground": "p:gray_os_fg",
          "invert_powerline": false,
          "leading_diamond": "",
          "min_width": 80,
          "style": "diamond",
          "template": "",
          "trailing_diamond": "",
          "type": "root"
        }
      ],
      "type": "rprompt"
    },
    {
      "alignment": "left",
      "newline": true,
      "segments": [
        {
          "foreground": "p:blue_primary",
          "min_width": 80,
          "style": "plain",
          "template": "╰─",
          "type": "text"
        },
        {
          "background": "transparent",
          "foreground": "p:blue_primary",
          "style": "diamond",
          "template": "{{ if .SSHSession }} {{ end }}{{ if .Root }}<p:blue_primary>─</><p:purple_session,transparent>{{ if eq .OS \"windows\" }}</><p:blue_primary>─</>{{ else }}<p:purple_session,transparent></><p:blue_primary>─</>{{ end }}{{ .UserName }}{{ end }}</>",
          "trailing_diamond": "<p:blue_primary></>",
          "type": "session"
        },
        {
          "foreground": "p:accent",
          "foreground_templates": [
            "{{ if gt .Code 0 }}p:red_alert{{ end }}"
          ],
          "properties": {
            "always_enabled": true
          },
          "style": "plain",
          "template": "",
          "type": "status"
        }
      ],
      "type": "prompt"
    }
  ],
  "console_title_template": "{{.Folder}}{{if .Root}} : root{{end}} : {{.Shell}}",
  "debug_prompt": {
    "background": "transparent",
    "foreground": "p:black",
    "template": "</><p:purple_session,transparent></><p:black,p:purple_session>  DEBUG </><p:purple_session,transparent></>"
  },
  "enable_cursor_positioning": true,
  "error_line": {
    "background": "transparent",
    "foreground": "p:black",
    "template": "<p:pink_error_line,transparent></>"
  },
  "final_space": true,
  "iterm_features": [
    "prompt_mark",
    "current_dir",
    "remote_host"
  ],
  "maps": {
    "host_name": {
      "Nick2bad4u2": "💻",
      "Nick2bad4u": "💻"
    },
    "user_name": {
      "Nick": "Nick",
      "Nick2bad4u": "Nick2bad4u",
      "Nick2": "Nick"
    },
    "shell_names": {
      "bash": "BASH",
      "cmd": "CMD",
      "fish": "FISH",
      "powershell": "PS",
      "ps": "PS",
      "pwsh": "PS",
      "zsh": "ZSH",
      "oh-my-posh": "OMP"
    }
  },
  "palette": {
    "npm_yellow": "#ffa07a",
    "magenta_copied": "#ff69b4",
    "blue_primary": "#00bfff",
    "accent": "#ff4500",
    "green_charging": "#5fdfef",
    "pink_storybook": "#ff69b4",
    "green_ahead": "#40e0d0",
    "yellow_discharging": "#ffb380",
    "prettier_yellow": "#ffa07a",
    "electron_red": "#ff4500",
    "typescript_blue": "#00bfff",
    "green_valid_line": "#006f7f",
    "windows_blue": "#00bfff",
    "gray_os": "#b0c4de",
    "gray_prompt_count_bg": "#1f1f2f",
    "orange_battery": "#ff7f50",
    "violet_project": "#9370db",
    "vite_yellow": "#ffa07a",
    "teal_sysinfo": "#20b2aa",
    "gray_os_fg": "#1a1a24",
    "tailwind_cyan": "#00ced1",
    "typescript_eslint_pink": "#ff69b4",
    "purple_session": "#9932cc",
    "pink_error_line": "#ff0000",
    "vitest_green": "#00ced1",
    "ipify_purple": "#9370db",
    "node_green": "#00ced1",
    "ipify_purple_v6": "#ba55d3",
    "react_cyan": "#00ffff",
    "orange": "#ff6347",
    "white": "#f0ffff",
    "pink_status_fail": "#dc143c",
    "green_success": "#00ced1",
    "gray_path_fg": "#2a2a34",
    "purple_ahead": "#9370db",
    "java_orange": "#ff8c00",
    "navy_text": "#0a0a14",
    "black": "#0a0a14",
    "orange_unmerged": "#ff4500",
    "python_blue": "#00bfff",
    "purple_exec": "#8b008b",
    "yellow_root_alt": "#ffb380",
    "red_deleted": "#dc143c",
    "cyan_renamed": "#00ffff",
    "maroon_error": "#8b0000",
    "green_help": "#20b2aa",
    "yellow_bright": "#ffa07a",
    "eslint_purple": "#9370db",
    "yellow_modified": "#ffa07a",
    "gray_prompt_count_fg": "#e0f7ff",
    "playwright_teal": "#00ced1",
    "npm_dark": "#0a0a14",
    "green_full": "#00ced1",
    "blue_tooltip": "#b0e0e6",
    "gray_untracked": "#708090",
    "blue_time": "#87ceeb",
    "axios_yellow": "#ffa07a",
    "prettier_black": "#0a0a14",
    "yellow_update": "#ffb380",
    "red_alert": "#ff4500",
    "python_yellow": "#ffa07a",
    "green_added": "#00ffff",
    "zustand_purple": "#9370db",
    "chart_teal": "#00ced1",
    "pink_weather": "#afeeee",
    "yellow_git_changed": "#ffd9b3"
  },
  "patch_pwsh_bleed": true,
  "pwd": "osc7",
  "secondary_prompt": {
    "background": "transparent",
    "foreground": "p:accent",
    "properties": {},
    "template": "╰─ "
  },
  "shell_integration": true,
  "tooltips": [
    {
      "background": "#61dafb",
      "foreground": "#ffffff",
      "leading_diamond": "",
      "properties": {
        "method": "GET",
        "url": "https://registry.npmjs.org/react/latest"
      },
      "style": "diamond",
      "template": " <b> React: v{{ .Body.version }}</b> ",
      "tips": [
        "react",
        "npm run dev"
      ],
      "trailing_diamond": "",
      "type": "http"
    },
    {
      "background": "#3178c6",
      "foreground": "#ffffff",
      "leading_diamond": "",
      "properties": {
        "method": "GET",
        "url": "https://registry.npmjs.org/typescript/latest"
      },
      "style": "diamond",
      "template": " <b>TS: v{{ .Body.version }}</b> ",
      "tips": [
        "typescript",
        "npm run type-check"
      ],
      "trailing_diamond": "",
      "type": "http"
    },
    {
      "background": "#f7b93e",
      "foreground": "#000000",
      "leading_diamond": "",
      "properties": {
        "method": "GET",
        "url": "https://registry.npmjs.org/vite/latest"
      },
      "style": "diamond",
      "template": " <b>⚡ Vite: v{{ .Body.version }}</b> ",
      "tips": [
        "vite",
        "npm run build"
      ],
      "trailing_diamond": "",
      "type": "http"
    },
    {
      "background": "#729b1b",
      "foreground": "#ffffff",
      "leading_diamond": "",
      "properties": {
        "method": "GET",
        "url": "https://registry.npmjs.org/vitest/latest"
      },
      "style": "diamond",
      "template": " <b>🧪 Vitest: v{{ .Body.version }}</b> ",
      "tips": [
        "vitest",
        "npm run test"
      ],
      "trailing_diamond": "",
      "type": "http"
    },
    {
      "background": "#47848f",
      "foreground": "#ffffff",
      "leading_diamond": "",
      "properties": {
        "method": "GET",
        "url": "https://registry.npmjs.org/playwright/latest"
      },
      "style": "diamond",
      "template": " <b>🎭 Playwright: v{{ .Body.version }}</b> ",
      "tips": [
        "playwright",
        "npm run test:e2e"
      ],
      "trailing_diamond": "",
      "type": "http"
    },
    {
      "background": "#06b6d4",
      "foreground": "#ffffff",
      "leading_diamond": "",
      "properties": {
        "method": "GET",
        "url": "https://registry.npmjs.org/tailwindcss/latest"
      },
      "style": "diamond",
      "template": " <b>🎨 Tailwind CSS: v{{ .Body.version }}</b> ",
      "tips": [
        "tailwindcss",
        "npm run lint:css"
      ],
      "trailing_diamond": "",
      "type": "http"
    },
    {
      "background": "#9b59d0",
      "foreground": "#000000",
      "leading_diamond": "",
      "properties": {
        "method": "GET",
        "url": "https://registry.npmjs.org/zustand/latest"
